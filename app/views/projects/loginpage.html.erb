<center>
<div class="login">
<div class="card">
<div class="card-block">
<h1>welcome to loginpage</h1>
<div class="container">
<%= form_with model: @create do|t|%>
    <div class="form-group">
    <%= t.label :username,"username"%>
    <%= t.text_field :username %><br><br>
    </div>

    <div class="form-group">
    <%= t.label :password,"password"%>
    <%= t.text_field :password %><br><br>
    </div>

    <div class="form-group">
    <%= t.label :email,"email"%>
    <%= t.text_field :email %><br><br>
    </div>

    <div class="form-group">
    <%= t.submit  %>
    </div>
    <% end %>
</div>
<div class="container" style="background-color:#f1f1f1">
</div>
</div>
</div>
</center>